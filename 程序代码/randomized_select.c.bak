#include<iostream>
#include<randomized_quicksort.h>

double RandomuzedPartition(double *A,p,r,i){
	if(p==r)
		return A[p];
		
	int q=RandomizedPartition(A,p,r);
	int k=q-p+1;
	
	if(i==k){
		return A[q];
	}else if(i<k){
		return RandomuzedPartition(A,p,q-1,i);
	}else{
		return RandomuzedPartition(A,q+1,r,i-k);
	}
}
